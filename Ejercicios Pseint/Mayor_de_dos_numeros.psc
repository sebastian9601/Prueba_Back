Algoritmo Mayor_de_dos_numeros
	Definir num1,num2 Como Real
	
	Escribir  "Ingrese el valor para el numero 1";
	leer num1;
	Escribir  "Ingrese el valor para el numero 2";
	leer num2;
	
	si(num1>num2)
		
		imprimir "El numero 1 es mayor que el numero 2";
		
	    sino
		FIN SI	
	si(num2>num1)	
			
			Escribir  "El numero 2 es mayor que el numero 1";	
			
    sino
						Escribir  "Los numero son iguales";	
			
			
	FIN SI		
